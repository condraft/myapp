<div class="contents">
  <div class="contents-row">
    <% @posts.each do |post| %>
      <%= render partial: "post", locals: { post: post } %>
    <% end %>
    <%= paginate(@posts) %>
  </div>
  <div class="contents-right">
    <a class="twitter-timeline" data-width="300" data-height="500" data-theme="light" data-link-color="#2B7BB9" href="https://twitter.com/ikumidddd?ref_src=twsrc%5Etfw">Tweets by ikumidddd</a> <script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>
  </div>
</div>