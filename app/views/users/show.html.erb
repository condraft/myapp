  <div class="container">
    <div class="row">
      <div class="col-sm-12 postlist jscroll">
         <p class="mypage"><%= @nickname %>のきろく</p>
        <% @posts.each do |post| %>
          <%= render partial: "posts/post", locals: { post: post } %>
        <% end %>
        <%= paginate(@posts) %>
      </div>
    </div>
  </div>